{
  "name": "banknifty-signals-backend",
  "version": "1.0.0",
  "description": "Backend for BankNifty Signals Workstation",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn src/index.ts",
  "dev:run": "node ./scripts/run_rules_demo.js",
  "dev:tsrun": "ts-node ./src/scripts/run_rules_demo.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:migrate": "ts-node src/db/migrations/001_init.sql",
    "db:seed": "ts-node src/db/seeders.ts",
    "test": "jest --coverage"
  ,
  "test:smoke": "node ./tests/check_rules_config.js",
  "run:dist": "node ./dist/scripts/run_rules_demo.js"
  },
  "engines": {
    "node": "18.18.2"
  },
  "dependencies": {
    "express": "4.18.2",
    "socket.io": "4.7.2",
    "pg": "8.11.1",
    "redis": "4.6.7",
  "technicalindicators": "^3.0.7",
  "typeorm": "0.3.17",
    "zod": "3.22.2",
    "pino": "8.17.0",
    "pino-pretty": "10.3.1",
    "uuid": "9.0.1",
    "ws": "8.14.2",
    "axios": "1.6.2",
    "crypto-js": "4.2.0",
    "cors": "2.8.5",
    "dotenv": "16.3.1",
    "reflect-metadata": "0.2.1"
  },
  "devDependencies": {
    "typescript": "5.2.2",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "jest": "29.7.0",
    "@types/express": "4.17.21",
    "@types/node": "20.10.6",
    "@types/socket.io": "3.0.2",
    "@types/ws": "8.5.10",
    "@types/crypto-js": "4.2.1",
    "@types/cors": "2.8.17",
    "@types/jest": "29.5.11",
    "@types/pino": "7.0.5",
    "@types/uuid": "9.0.7"
  }
}
